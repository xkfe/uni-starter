{
  "name": "@xkfe/uni-starter",
  "type": "commonjs",
  "version": "0.1.0",
  "packageManager": "pnpm@9.2.0",
  "description": "uniapp的快速开发模板,包含多种开发场景实践演示",
  "author": "xkfe <xkfe0516@gmail.com>",
  "license": "MIT",
  "keywords": [
    "template",
    "uniapp",
    "vue"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "postinstall": "pnpm simple-git-hooks",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "dev:app": "uni -p app",
    "dev:app-android": "uni -p app-android",
    "dev:app-ios": "uni -p app-ios",
    "dev:custom": "uni -p",
    "dev:h5": "uni",
    "dev:h5:ssr": "uni --ssr",
    "dev:mp-alipay": "uni -p mp-alipay",
    "dev:mp-baidu": "uni -p mp-baidu",
    "dev:mp-jd": "uni -p mp-jd",
    "dev:mp-kuaishou": "uni -p mp-kuaishou",
    "dev:mp-lark": "uni -p mp-lark",
    "dev:mp-qq": "uni -p mp-qq",
    "dev:mp-toutiao": "uni -p mp-toutiao",
    "dev:mp-weixin": "uni -p mp-weixin",
    "dev:mp-xhs": "uni -p mp-xhs",
    "dev:quickapp-webview": "uni -p quickapp-webview",
    "dev:quickapp-webview-huawei": "uni -p quickapp-webview-huawei",
    "dev:quickapp-webview-union": "uni -p quickapp-webview-union",
    "build:app": "uni build -p app",
    "build:app-android": "uni build -p app-android",
    "build:app-ios": "uni build -p app-ios",
    "build:custom": "uni build -p",
    "build:h5": "uni build",
    "build:h5:ssr": "uni build --ssr",
    "build:mp-alipay": "uni build -p mp-alipay",
    "build:mp-baidu": "uni build -p mp-baidu",
    "build:mp-jd": "uni build -p mp-jd",
    "build:mp-kuaishou": "uni build -p mp-kuaishou",
    "build:mp-lark": "uni build -p mp-lark",
    "build:mp-qq": "uni build -p mp-qq",
    "build:mp-toutiao": "uni build -p mp-toutiao",
    "build:mp-weixin": "uni build -p mp-weixin",
    "build:mp-xhs": "uni build -p mp-xhs",
    "build:quickapp-webview": "uni build -p quickapp-webview",
    "build:quickapp-webview-huawei": "uni build -p quickapp-webview-huawei",
    "build:quickapp-webview-union": "uni build -p quickapp-webview-union",
    "type-check": "vue-tsc --noEmit"
  },
  "dependencies": {
    "@dcloudio/uni-app": "3.0.0-4010520240507001",
    "@dcloudio/uni-app-plus": "3.0.0-4010520240507001",
    "@dcloudio/uni-components": "3.0.0-4010520240507001",
    "@dcloudio/uni-h5": "3.0.0-4010520240507001",
    "@dcloudio/uni-mp-alipay": "3.0.0-4010520240507001",
    "@dcloudio/uni-mp-baidu": "3.0.0-4010520240507001",
    "@dcloudio/uni-mp-jd": "3.0.0-4010520240507001",
    "@dcloudio/uni-mp-kuaishou": "3.0.0-4010520240507001",
    "@dcloudio/uni-mp-lark": "3.0.0-4010520240507001",
    "@dcloudio/uni-mp-qq": "3.0.0-4010520240507001",
    "@dcloudio/uni-mp-toutiao": "3.0.0-4010520240507001",
    "@dcloudio/uni-mp-weixin": "3.0.0-4010520240507001",
    "@dcloudio/uni-mp-xhs": "3.0.0-4010520240507001",
    "@dcloudio/uni-quickapp-webview": "3.0.0-4010520240507001",
    "@uni-helper/uni-network": "^0.18.1",
    "@uni-helper/uni-promises": "^0.2.1",
    "@uni-helper/uni-use": "^0.19.12",
    "@unocss/reset": "~0.58.9",
    "@vueuse/core": "9",
    "nutui-uniapp": "^1.7.10",
    "pinia": "^2.1.7",
    "pinia-plugin-persist-uni": "^1.3.1",
    "vue": "^3.4.21",
    "vue-i18n": "^9.1.9",
    "wot-design-uni": "^1.2.28"
  },
  "devDependencies": {
    "@dcloudio/types": "^3.4.8",
    "@dcloudio/uni-automator": "3.0.0-4010520240507001",
    "@dcloudio/uni-cli-shared": "3.0.0-4010520240507001",
    "@dcloudio/uni-stacktracey": "3.0.0-4010520240507001",
    "@dcloudio/vite-plugin-uni": "3.0.0-4010520240507001",
    "@uni-helper/eslint-config": "^0.0.6",
    "@uni-helper/uni-app-types": "^0.5.13",
    "@uni-helper/unocss-preset-uni": "^0.2.9",
    "@uni-helper/vite-plugin-uni-components": "^0.0.9",
    "@uni-helper/vite-plugin-uni-layouts": "^0.1.10",
    "@uni-helper/vite-plugin-uni-manifest": "^0.2.6",
    "@uni-helper/vite-plugin-uni-middleware": "^0.0.1",
    "@uni-helper/vite-plugin-uni-pages": "^0.2.24",
    "@unocss/eslint-plugin": "~0.58.9",
    "@unocss/transformer-directives": "~0.58.9",
    "@unocss/transformer-variant-group": "~0.58.9",
    "@vue/runtime-core": "^3.4.21",
    "@vue/tsconfig": "^0.1.3",
    "commitizen": "^4.3.0",
    "cz-git": "^1.9.3",
    "eslint": "^9.6.0",
    "eslint-plugin-format": "^0.1.2",
    "lint-staged": "^15.2.7",
    "sass": "^1.77.6",
    "simple-git-hooks": "^2.11.1",
    "typescript": "^5.5.2",
    "unocss": "~0.58.9",
    "unocss-applet": "^0.8.2",
    "unplugin-auto-import": "^0.17.6",
    "vite": "5.2.8",
    "vue-tsc": "^1.0.24"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged",
    "commit-msg": "node scripts/verify-commit.mjs"
  },
  "lint-staged": {
    "*": "eslint --fix"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  }
}
